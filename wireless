config wifi-device  radio0
	option type     mac80211
	option channel  4
	option hwmode	11g
	option path	'platform/ar934x_wmac'
	option htmode	HT40+

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     imceo,bitch
	option encryption psk2
	option key	ssfour315

